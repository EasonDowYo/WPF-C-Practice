        </TreeView>
        <Button Content="Button" HorizontalAlignment="Left" Height="58" Margin="299,36,0,0" VerticalAlignment="Top" Width="118" Click="Button_Click"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="353,148,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>

