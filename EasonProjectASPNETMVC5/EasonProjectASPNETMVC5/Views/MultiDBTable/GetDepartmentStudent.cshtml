@model EasonProjectASPNETMVC5.Models.DepartmentTable

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>GetDepartmentStudent</title>
</head>
<body>
    <div>
        <h4>DepartmentTable</h4>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.DepartmentName)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.DepartmentName)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.DepartmentYear)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.DepartmentYear)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.College)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.College)
            </dd>

        </dl>
    </div>
    <p>
        @Html.ActionLink("Edit", "Edit", new { id = Model.DepartmentID }) |
        @Html.ActionLink("Back to List", "Index")
    </p>

    <table>
        <tr>
            <th>StudentID</th>
            <th>StudentName</th>
        </tr>
        @foreach (var item in Model.StudentTable)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => item.StudentID)</td>
                <td>@Html.DisplayFor(modelItem => item.StudentName)</td>
            </tr>
        }
    </table>





</body>
</html>
